{
    "compilerOptions": {
      "target": "es5",                          
      "module": "commonjs",                     
      "outDir": "./dist",                       
      "rootDir": "./src",                       
      "strict": true,                           
      "esModuleInterop": true,                  
      "skipLibCheck": true,                     
      "forceConsistentCasingInFileNames": true  
    },
    "include": [
    "src/**/*",
    "swagger-jsdoc.d.ts"
    ],
    "exclude": ["node_modules", "dist"]
  }
  